<% provide(:title, "New Invitation") %>
<%= form_for @invitation, url: admin_invitations_path do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <div class="form-group">
    <%= f.label :recipient_email, 'Email address' %>
    <%= f.text_field :recipient_email %>
  </div>
  <%= f.submit %>
<% end %>

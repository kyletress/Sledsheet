<%= form_for [:admin, @track] do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :latitude %>
    <%= f.text_field :latitude, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :longitude %>
    <%= f.text_field :longitude, class: 'form-control' %>
  </div>
  <%= f.submit %>

<% end %>
